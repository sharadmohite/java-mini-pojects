//A Grade Calculator is a handy tool that helps determine a studentâ€™s final grade based on scores from exams, assignments, or other components.
import java.util.Scanner;
class Test
{
	public static void main(String[] args)
	{
		double score;
		char grade;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the score you got in test between 0-100");
		score=sc.nextDouble();

		if(score>=90)
		{
		  grade='A';
		}
		else if(score>=80)
		{
		  grade='B';
		}
		else if(score>=70)
		{
		  grade='C';
		}
		else if(score>=60)
		{
		  grade='D';
		}
		else
		{
			grade='F';
		}

		System.out.println("your grade is :"+grade);
	
		
	}
}
